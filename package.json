{
  "name": "markdown-it-vitepress-demo",
  "type": "module",
  "version": "0.1.11",
  "packageManager": "pnpm@9.1.4",
  "description": "",
  "author": "Hairyf <wwu710632@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/markdown-it-vitepress-demos#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/markdown-it-vitepress-demos.git"
  },
  "bugs": "https://github.com/hairyf/markdown-it-vitepress-demos/issues",
  "keywords": [
    "markdown",
    "vitepress",
    "demo"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./markdown-it": {
      "types": "./dist/markdown-it.d.ts",
      "require": "./dist/markdown-it.cjs",
      "import": "./dist/markdown-it.js"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nodemon -w src/ -e .ts -x unbuild --stub",
    "play": "npm run docs:dev",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run build && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@vue/compiler-core": "^3.4.27",
    "@vue/compiler-sfc": "^3.4.27",
    "esbuild": "^0.21.4",
    "fs-extra": "^11.2.0",
    "markdown-it-container": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.0.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it-container": "^2.0.10",
    "@types/node": "^20.12.13",
    "bumpp": "^9.4.1",
    "eslint": "^8.37.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.2",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.12",
    "vitepress": "1.0.0-beta.5",
    "vitest": "^1.6.0",
    "vue": "^3.4.27"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
